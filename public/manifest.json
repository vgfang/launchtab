{
  "manifest_version": 3,
  "name": "launchtab",
  "version": "1.0.2",
  "description": "New tab replacement for quick link navigation using VIM-style keychords.",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": ["storage"],
  "content_security_policy": {
    "extension_pages": "script-src 'self' http://localhost:3000; object-src 'self'"
  },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "32": "icon.png",
    "128": "icon.png"
  }
}
